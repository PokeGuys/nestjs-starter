{
  "name": "nestjs-starter",
  "version": "0.0.1",
  "private": true,
  "description": "The starter kit of NestJS",
  "homepage": "https://github.com/PokeGuys/nestjs-starter-kit#readme",
  "bugs": {
    "url": "https://github.com/PokeGuys/nestjs-starter-kit/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PokeGuys/nestjs-starter-kit.git"
  },
  "license": "MIT",
  "author": "PokeGuys",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "lint:check": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "prepare": "is-ci || husky install",
    "start": "nest start",
    "start:debug": "nest start --debug --watch",
    "start:dev": "nest start --watch",
    "start:prod": "node dist/main",
    "swagger:build": "pnpx nestjs-command generate:swagger",
    "test": "pnpm run test:unit && pnpm run test:e2e",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:e2e:cov": "jest --config ./test/jest-e2e.json --coverage",
    "test:e2e:watch": "jest --config ./test/jest-e2e.json --watch",
    "test:unit": "jest",
    "test:unit:cov": "jest --coverage",
    "test:unit:watch": "jest --watch"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "3.95.0",
    "@mikro-orm/core": "5.1.4",
    "@mikro-orm/nestjs": "5.0.1",
    "@mikro-orm/postgresql": "5.1.4",
    "@nestjs/common": "8.4.5",
    "@nestjs/config": "2.0.1",
    "@nestjs/core": "8.4.5",
    "@nestjs/mongoose": "9.1.0",
    "@nestjs/platform-fastify": "8.4.5",
    "@nestjs/swagger": "5.2.1",
    "@nestjs/terminus": "8.0.6",
    "class-transformer": "0.5.1",
    "class-validator": "0.13.2",
    "date-fns": "2.28.0",
    "fastify": "3.29.0",
    "fastify-swagger": "5.2.0",
    "firebase-admin": "10.2.0",
    "ioredis": "5.0.5",
    "ioredis-mock": "8.2.0",
    "lodash.pick": "4.4.0",
    "mongoose": "6.3.4",
    "nestjs-command": "3.1.1",
    "nestjs-i18n": "9.0.13",
    "nestjs-pino": "2.6.0",
    "pino": "7.11.0",
    "pino-http": "6.6.0",
    "redis-commands": "1.7.0",
    "redlock": "4.2.0",
    "reflect-metadata": "0.1.13",
    "rimraf": "3.0.2",
    "rxjs": "7.5.5",
    "uuid": "8.3.2",
    "yargs": "17.5.1"
  },
  "devDependencies": {
    "@commitlint/cli": "17.0.0",
    "@commitlint/config-angular": "17.0.0",
    "@commitlint/config-conventional": "17.0.0",
    "@nestjs/cli": "8.2.6",
    "@nestjs/schematics": "8.0.11",
    "@nestjs/testing": "8.4.5",
    "@types/jest": "27.5.1",
    "@types/lodash.pick": "4.4.7",
    "@types/node": "16.11.36",
    "@types/supertest": "2.0.12",
    "@types/uuid": "8.3.4",
    "@types/yargs": "17.0.10",
    "@typescript-eslint/eslint-plugin": "5.26.0",
    "@typescript-eslint/parser": "5.26.0",
    "eslint": "8.16.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-airbnb-typescript": "17.0.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jest": "26.2.2",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-promise": "6.0.0",
    "husky": "8.0.1",
    "is-ci": "3.0.1",
    "jest": "28.1.0",
    "lint-staged": "12.4.2",
    "prettier": "2.6.2",
    "sort-package-json": "1.57.0",
    "source-map-support": "0.5.21",
    "supertest": "6.2.3",
    "ts-jest": "28.0.3",
    "ts-node": "10.8.0",
    "tsconfig-paths": "4.0.0",
    "typescript": "4.6.4"
  },
  "engines": {
    "node": ">=16.0.0",
    "pnpm": ">=6"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "@firebase/app-compat",
        "@firebase/app-types"
      ]
    }
  }
}

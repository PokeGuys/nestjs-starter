{
  "name": "nestjs-starter",
  "version": "0.0.1",
  "private": true,
  "description": "The starter kit of NestJS",
  "homepage": "https://github.com/PokeGuys/nestjs-starter-kit#readme",
  "bugs": {
    "url": "https://github.com/PokeGuys/nestjs-starter-kit/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PokeGuys/nestjs-starter-kit.git"
  },
  "license": "MIT",
  "author": "PokeGuys",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "prepare": "is-ci || husky install",
    "start": "nest start",
    "start:debug": "nest start --debug --watch",
    "start:dev": "nest start --watch",
    "start:prod": "node dist/main",
    "swagger:build": "pnpx nestjs-command generate:swagger",
    "test": "jest",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "3.44.0",
    "@mikro-orm/core": "4.5.9",
    "@mikro-orm/nestjs": "4.3.1",
    "@mikro-orm/postgresql": "4.5.9",
    "@nestjs/common": "8.2.3",
    "@nestjs/config": "1.1.5",
    "@nestjs/core": "8.2.3",
    "@nestjs/mongoose": "9.0.1",
    "@nestjs/platform-fastify": "8.2.3",
    "@nestjs/swagger": "5.1.5",
    "@nestjs/terminus": "8.0.3",
    "class-transformer": "0.5.1",
    "class-validator": "0.13.1",
    "date-fns": "^2.27.0",
    "fastify": "3.24.1",
    "fastify-swagger": "4.12.6",
    "firebase-admin": "10.0.0",
    "ioredis": "4.28.2",
    "ioredis-mock": "5.8.1",
    "mongoose": "6.1.2",
    "nestjs-command": "3.1.0",
    "nestjs-i18n": "8.2.2",
    "redis-commands": "1.7.0",
    "redlock": "4.2.0",
    "reflect-metadata": "0.1.13",
    "rimraf": "3.0.2",
    "rxjs": "7.4.0",
    "uuid": "8.3.2",
    "yargs": "17.3.0"
  },
  "devDependencies": {
    "@commitlint/cli": "15.0.0",
    "@commitlint/config-angular": "^15.0.0",
    "@commitlint/config-conventional": "15.0.0",
    "@nestjs/cli": "8.1.6",
    "@nestjs/schematics": "8.0.5",
    "@nestjs/testing": "8.2.3",
    "@types/ioredis": "4.28.3",
    "@types/jest": "27.0.3",
    "@types/node": "16.11.12",
    "@types/supertest": "2.0.11",
    "@types/uuid": "8.3.3",
    "@types/yargs": "17.0.7",
    "@typescript-eslint/eslint-plugin": "5.6.0",
    "@typescript-eslint/parser": "5.6.0",
    "eslint": "8.4.1",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-airbnb-typescript": "16.1.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.25.3",
    "eslint-plugin-jest": "25.3.0",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-promise": "5.2.0",
    "husky": "7.0.4",
    "import-sort-cli": "6.0.0",
    "import-sort-parser-typescript": "6.0.0",
    "is-ci": "3.0.1",
    "jest": "27.4.4",
    "lint-staged": "12.1.2",
    "prettier": "2.5.1",
    "sort-package-json": "1.53.1",
    "source-map-support": "0.5.21",
    "supertest": "6.1.6",
    "ts-jest": "27.1.1",
    "ts-node": "10.4.0",
    "tsconfig-paths": "3.12.0",
    "typescript": "4.5.4"
  },
  "engines": {
    "node": ">=16.0.0",
    "pnpm": ">=6"
  }
}

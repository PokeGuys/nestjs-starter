{
  "name": "nestjs-starter",
  "version": "0.0.1",
  "private": true,
  "description": "The starter kit of NestJS",
  "homepage": "https://github.com/PokeGuys/nestjs-starter-kit#readme",
  "bugs": {
    "url": "https://github.com/PokeGuys/nestjs-starter-kit/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PokeGuys/nestjs-starter-kit.git"
  },
  "license": "MIT",
  "author": "PokeGuys",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "coverage:merge": "pnpm run test:unit:cov && pnpm run test:e2e:cov && nyc merge .nyc_output coverage.json",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "lint:check": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "prepare": "is-ci || husky install",
    "start": "nest start",
    "start:debug": "nest start --debug --watch",
    "start:dev": "nest start --watch",
    "start:prod": "node dist/main",
    "swagger:build": "pnpx nestjs-command generate:swagger",
    "test": "pnpm run test:unit && pnpm run test:e2e",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:e2e:cov": "jest --config ./test/jest-e2e.json --coverage",
    "test:e2e:watch": "jest --config ./test/jest-e2e.json --watch",
    "test:unit": "jest",
    "test:unit:cov": "jest --coverage",
    "test:unit:watch": "jest --watch"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "3.49.0",
    "@mikro-orm/core": "4.5.10",
    "@mikro-orm/nestjs": "4.3.1",
    "@mikro-orm/postgresql": "4.5.10",
    "@nestjs/common": "8.2.6",
    "@nestjs/config": "1.1.6",
    "@nestjs/core": "8.2.6",
    "@nestjs/mongoose": "9.0.2",
    "@nestjs/platform-fastify": "8.2.6",
    "@nestjs/swagger": "5.2.0",
    "@nestjs/terminus": "8.0.4",
    "class-transformer": "0.5.1",
    "class-validator": "0.13.2",
    "date-fns": "2.28.0",
    "fastify": "3.27.0",
    "fastify-swagger": "4.13.1",
    "firebase-admin": "10.0.2",
    "ioredis": "4.28.3",
    "ioredis-mock": "7.0.0",
    "lodash.pick": "4.4.0",
    "mongoose": "6.1.8",
    "nestjs-command": "3.1.1",
    "nestjs-i18n": "8.2.2",
    "nestjs-pino": "2.5.0",
    "pino": "7.6.5",
    "pino-http": "6.6.0",
    "redis-commands": "1.7.0",
    "redlock": "4.2.0",
    "reflect-metadata": "0.1.13",
    "rimraf": "3.0.2",
    "rxjs": "7.5.2",
    "uuid": "8.3.2",
    "yargs": "17.3.1"
  },
  "devDependencies": {
    "@commitlint/cli": "16.1.0",
    "@commitlint/config-angular": "16.0.0",
    "@commitlint/config-conventional": "16.0.0",
    "@nestjs/cli": "8.2.0",
    "@nestjs/schematics": "8.0.5",
    "@nestjs/testing": "8.2.6",
    "@types/ioredis": "4.28.7",
    "@types/jest": "27.4.0",
    "@types/lodash.pick": "4.4.6",
    "@types/node": "16.11.22",
    "@types/supertest": "2.0.11",
    "@types/uuid": "8.3.4",
    "@types/yargs": "17.0.8",
    "@typescript-eslint/eslint-plugin": "5.10.2",
    "@typescript-eslint/parser": "5.10.2",
    "eslint": "8.8.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-airbnb-typescript": "16.1.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-jest": "26.0.0",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-promise": "6.0.0",
    "husky": "7.0.4",
    "is-ci": "3.0.1",
    "jest": "27.4.7",
    "lint-staged": "12.3.3",
    "nyc": "15.1.0",
    "prettier": "2.5.1",
    "sort-package-json": "1.53.1",
    "source-map-support": "0.5.21",
    "supertest": "6.2.2",
    "ts-jest": "27.1.3",
    "ts-node": "10.4.0",
    "tsconfig-paths": "3.12.0",
    "typescript": "4.5.5"
  },
  "engines": {
    "node": ">=16.0.0",
    "pnpm": ">=6"
  }
}
